{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node ../tools/build-garment-catalog.mjs --out frontend/src/assets/garments/garments.generated.json",
    "dev": "run-p catalog:watch dev:vite",
    "dev:vite": "vite",
    "catalog:generate": "node ../tools/build-garment-catalog.mjs --out frontend/src/assets/garments/garments.generated.json",
    "catalog:watch": "node --watch ../tools/build-garment-catalog.mjs --watch-path ./src/assets/garments/meta --watch-path ./src/assets/garments/anchors --out frontend/src/assets/garments/garments.generated.json",
    "build": "npm run catalog:generate && tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,html,json}\"",
    "preview": "vite preview",
    "typecheck": "npm run catalog:generate && tsc -b --pretty false",
    "test": "vitest run"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "three": "^0.180.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  }
}
